    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "dist/",
    "paths": {
      "@/*": ["src/*"],
    },
    "sourceMap": true,
    "target": "es6"
  },
  "include": [
    "src",
    ".eslintrc.js",
    "webpack.config.js",
    "react-table-config.d.ts"
  ]
}